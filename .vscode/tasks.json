{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"command": "zsh",
	"isShellCommand":true,
	"showOutput":"always",
	"options": {
		"cwd": "${workspaceRoot}"
	},
	"args": [
		"-c"
	],
	"tasks": [
		{
			"taskName": "build",
			"suppressTaskName": true,
			"isBuildCommand": true,
			"args": [
				"make build"
			],
			"problemMatcher": [
				"$gcc"
			]
		}
	]
}